ul.results
  padding 0

  li
    list-style-type none
    display inline-block
    padding-right 1em
    width 100% 
    margin-bottom 2em
    position relative
    font-size 85%

    table
      background-color #ddd
      width 25%
      float left
    
      tr.heading
        background-color black
        color orange
      td
        padding 0.5em
        text-align center

      li
        width auto
    
    .round-chart
      width 35% 
      float left


.positive
  color green
  font-weight bold

.negative
  color red
  font-weight bold